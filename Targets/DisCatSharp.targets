<?xml version="1.0" encoding="utf-8"?>

<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<Authors>AITSYS, DisCatSharp, DisCatSharp Contributors</Authors>
		<Company>AITSYS</Company>
		<GeneratePackageOnBuild>False</GeneratePackageOnBuild>
		<PackageProjectUrl>https://ext-docs.dcs.aitsys.dev</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Aiko-IT-Systems/DisCatSharp.Extensions</RepositoryUrl>
		<RepositoryType>Git</RepositoryType>
		<PackageIcon>logo-pride.png</PackageIcon>
		<PackageLicenseExpression />
		<PackageLicenseFile>LICENSE.md</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE.md">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
	<ItemGroup>
		<None Include="..\logo-pride.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
</Project>
