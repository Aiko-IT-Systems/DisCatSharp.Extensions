			}
		},
		"template": ["default", "modern", "dcs"],
		"postProcessors": ["ExtractSearchIndex"],
		"noLangKeyword": false,
		"keepFileLink": true,
		"sitemap": {
			"baseUrl": "https://ext-docs.dcs.aitsys.dev/",
			"changefreq": "daily",
			"priority": 1.0
		},
		"markdownEngineProperties": {
			"markdigExtensions": ["attributes"]
		}
