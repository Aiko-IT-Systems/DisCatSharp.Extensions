{
	"metadata":[
		{
			"src":[
				{
					"src":"../DisCatSharp.Extensions.TwoFactorCommands/",
					"files":[
						"**.csproj"
					],
					"exclude":[
						"**/obj/**",
						"**/bin/**"
					]
				}
			],
			"dest":"api/DisCatSharp.Extensions.TwoFactorCommands",
			"filter":"filter_config.yml",
			"disableDefaultFilter":false
		}
	],
	"build":{
		"xref": [
			"https://backup-docs.dcs.aitsys.dev/xrefmap.yml"
		],
		"content":[
			{
				"files":[
					"api/**/**.yml",
					"api/**/**.md",
					"api/**.yml",
					"api/**.md"
				]
			},
			{
				"files":[
					"**.md",
					"toc.yml",
					"articles/**.yml",
					"articles/**.md"
				],
				"exclude":[
					"**/bin/**",
					"**/obj/**",
					"_site/**",
					"dcs/**"
				]
			}
		],
		"resource":[
			{
				"files":[
					"images/**",
					"natives/**.zip",
					"./.htaccess"
				],
				"exclude":[
					"**/bin/**",
					"**/obj/**",
					"_site/**",
					"images/_**"
				]
			}
		],
		"overwrite":[],
		"dest":"_site",
		"globalMetadata":{
			"_appTitle": "DisCatSharp Extensions Docs",
			"_appName": "DisCatSharp Extensions",
			"_appFooter": "<strong>Made with ❤️ by AITSYS</strong>",
			"_appLogoPath": "logo.png",
			"_enableSearch": true,
			"_disableNextArticle": false,
			"_disableSideFilter": false,
			"_disableTocFilter": false,
			"_disableNewTab": false,
			"_disableContribution": false,
			"_disableBreadcrumb": false,
			"_gitUrlPattern": "git",
			"_gitContribute": {
				"repo": "https://github.com/Aiko-IT-Systems/DisCatSharp.Extensions",
				"branch": "main"
			},
			"_googleTagId": "G-8JHG7YVNKK",
			"_lang": "en-US",
			"_ogType": "website",
			"_ogImage": "https://cdn.aitsys.dev/file/data/kmjpa6f64me66dsm7dz3/PHID-FILE-degpfzd7nbw2q5yko5j7/logobig.png",
			"_fbPageId": "112155734832129",
			"_twitterSite": "@DisCatSharp",
			"_twitterImage": "https://cdn.aitsys.dev/file/data/kmjpa6f64me66dsm7dz3/PHID-FILE-degpfzd7nbw2q5yko5j7/logobig.png",
			"_layout": "conceptual"
		},
		"disableGitFeatures": false,
		"exportRawModel": false,
		"exportViewModel": false,
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
