			"_ogType": {
				"articles/**/*.md": "article",
				"changelogs/**/*.md": "article"
			}
		},
		"template": [
			"default",
			"modern",
			"dcs"
		],
		"postProcessors": [
			"ExtractSearchIndex"
		],
		"noLangKeyword":false,
		"keepFileLink":false,
		"cleanupCacheHistory":false,
		"sitemap":{
			"baseUrl":"https://ext-docs.dcs.aitsys.dev/",
			"changefreq":"daily",
			"priority":1.0
		}
